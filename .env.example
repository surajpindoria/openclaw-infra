# --- AI Provider Keys ---
# At least one provider key is required for OpenClaw to start
ANTHROPIC_API_KEY=your_key_here
# OPENAI_API_KEY=your_key_here

# --- Authentication & Security ---
# Use 'admin' as default username unless AUTH_USERNAME is set
AUTH_PASSWORD=choose_a_strong_password
OPENCLAW_GATEWAY_TOKEN=generate_a_random_hex_string

# --- Network (Trusted Proxies for Mac/OrbStack) ---
OPENCLAW_GATEWAY_TRUSTED_PROXIES=127.0.0.1,192.168.97.1,host.docker.internal

# --- Notifications ---
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_ALLOW_FROM=your_telegram_user_id
